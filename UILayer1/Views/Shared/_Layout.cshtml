<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - UILayer1</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
    <link rel="stylesheet" href="~/css/SideBar.css" />
    <link rel="stylesheet" href="~/css/navigation-bar.css" />

</head>
<body>
    <header>
        <nav class="navbar navbar-default navbar-fixed-top">
            <div class="navbar-brand ">
                MOBIZONE
            </div>
            <div class="navbar carousel-item-right">
                Admin
            </div>
        </nav>
    </header>
    <div class="" style="padding-top:60px;">
        <div class="sidebar" >
        <a class="" asp-action="Index">Dashboard</a>
        <a class="active" asp-action="Index">Products</a>
        <a asp-action="">Users</a>
        <a asp-action="">Orders</a>
        <a asp-action="">Settings</a>
    </div>
     
    <div class="content " >
        <div class="container-fluid">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
        </div>
    </div>
    </div>
   
    

    

    <footer class="border-top footer text-muted">
        
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
